{
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"browsers": [
						"last 2 versions",
						"ios_saf >= 8",
						"not IE <= 10",
						"chrome >= 49",
						"firefox >= 49",
						"> 1%"
					]
				},
				"debug": false,
				"loose": true,
				"modules": false
			}
		]
	],
	"plugins": [
		["transform-react-jsx", { "pragma": "h" }]
	]
}
